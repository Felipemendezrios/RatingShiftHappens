@article{bourgaultPhysicallyBasedMethod2020,
  title = {A {{Physically Based Method}} for {{Real}}-{{Time Monitoring}} of {{Tidal River Discharges From Water Level Observations}}, {{With}} an {{Application}} to the {{St}}. {{Lawrence River}}},
  author = {Bourgault, Daniel and Matte, Pascal},
  year = {2020},
