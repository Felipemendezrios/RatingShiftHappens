@article{pujolMultidimensionalHydrologicalHydraulic2022,
  title = {Multi-Dimensional Hydrological--Hydraulic Model with Variational Data Assimilation for River Networks and Floodplains},
  author = {Pujol, L{\'e}o and Garambois, Pierre-Andr{\'e} and Monnier, J{\'e}r{\^o}me},
  year = {2022},
